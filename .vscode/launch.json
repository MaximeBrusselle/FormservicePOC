{
  "version": "0.2.0",
  "configurations": [
    {
      "command": "npm run dev",
      "name": "Development Server",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "name": "Development Client",
      "type": "firefox",
      "request": "launch",
      "url": "http://localhost:4321",
      "webRoot": "${workspaceFolder}/src",
      "skipFiles": [
        "**/node_modules/**"
      ],
    }
  ]
}
